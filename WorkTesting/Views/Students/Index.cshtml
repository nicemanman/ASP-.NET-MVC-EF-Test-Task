@model IEnumerable<WorkTesting.Models.StudentInGroup>

    <p>
       
            @Html.ActionLink("Добавить студента в группу", "Create", "Students", new { id = 1})
        
        </p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Staff.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Staff.Organisations.Name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Staff.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Staff.Organisations.Name)
            </td>
            <td>
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
